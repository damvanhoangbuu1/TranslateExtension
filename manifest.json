{
    "manifest_version": 3,
    "name": "Đại Ca Extension",
    "version": "1.0",
    "description": "Extension đầu tay của đại ca!",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "128": "icon128.png"
        }
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["scripts/content.js"],
            "run_at": "document_end"
        }
    ]
}